  "overrides": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "comments": {
    "_dependencies_notes": "DEPENDENCY UPGRADE NOTES - These cannot be stored in package.json directly, see DEPENDENCIES.md",
    "_upgraded": [
      "react: 16.x -> 18.x (React 19 incompatible with react-notifications)",
      "react-dom: 16.x -> 18.x (matches React)",
      "@mui/material: 5.x -> 7.x (skipped 6.x, direct to latest)",
      "@mui/icons-material: 5.x -> 7.x (matches MUI)",
      "@testing-library/react: 14.x (React 18 compatible)",
      "eslint: 8.x -> 9.x (new flat config format)",
      "mathjs: 12.x -> 14.x (performance improvements)",
      "css-loader: 6.x -> 7.x (webpack 5 optimizations)",
      "style-loader: 3.x -> 4.x (webpack 5 optimizations)",
      "copy-webpack-plugin: 11.x -> 13.x (webpack 5 optimizations)",
      "webpack-cli: 5.x -> 6.x (latest)",
      "webpack-dev-server: 4.x -> 5.x (latest)",
      "babel-loader: 9.x -> 10.x (latest)",
      "chai: 4.x -> 5.x (ESM native)",
      "uuid: 9.x -> 11.x (latest)",
      "solhint: 4.x -> 5.x (improved rules)"
    ],
    "_removed": [
      "truffle: Removed - deprecated, migrated to Foundry/Forge",
      "ganache: Removed - deprecated, use Anvil (Foundry) instead",
      "web3.js: Removed - replaced with ethers.js v6",
      "@truffle/contract: Removed - using ethers.js Contract directly",
      "ethereum-ens: Removed - using ethers.js built-in ENS support",
      "jquery: Removed - using native fetch API",
      "etherscan-api: Removed - old unmaintained package",
      "bluebird: Removed - native Promises are sufficient",
      "coveralls: Removed - deprecated, use alternatives",
      "eth-gas-reporter: Removed - requires truffle",
      "@digix/tempo: Removed - deprecated testing utility"
    ],
    "_cannot_upgrade": [
      "react-notifications: 1.7.4 is latest, unmaintained since 2020",
      "event-emitter: 0.3.5 is latest, stable and works",
      "file-loader: 6.2.0 is latest, webpack 5 native approach preferred",
      "url-loader: 4.1.1 is latest, webpack 5 asset modules preferred",
      "identity-obj-proxy: 3.0.0 is latest, stable for CSS mocking",
      "solhint-plugin-prettier: 0.1.0 is latest, minimal maintenance",
      "yargs: 17.7.2 is last CommonJS version; v18+ is ESM-only",
      "@ensdomains/ens: 0.6.2 uses Solidity 0.7.x; we use pre-built artifacts to avoid recompilation",
      "@ensdomains/resolver: 0.3.1 uses Solidity 0.7.x; we use pre-built artifacts to avoid recompilation"
    ],
    "_breaking_changes": [
      "ESLint 9 requires new flat config format (eslint.config.mjs)",
      "MUI 7 has theming and component API changes",
      "React 18 uses createRoot API (already migrated)",
      "chai 5 is ESM-only"
    ],
    "_blocked_upgrades": [
      "React 19: blocked by react-notifications using deprecated ReactDOM.findDOMNode()",
      "@testing-library/react 16: requires React 19"
    ]
  }
}
