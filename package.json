{
  "name": "blockparty",
  "version": "1.0.0",
  "description": "Event registration DApp with deposit-based attendance incentives",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:js": "eslint 'src/**/*.js' 'scripts/**/*.js' --fix",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:check": "eslint 'src/**/*.js' && solhint 'contracts/**/*.sol'",
    "format": "prettier --write 'src/**/*.js' 'scripts/**/*.js' 'contracts/**/*.sol'",
    "build": "npm run lint:check && webpack --mode production",
    "forge:build": "forge build",
    "forge:test": "forge test",
    "forge:test:verbose": "forge test -vvv",
    "forge:coverage": "forge coverage",
    "test": "forge test",
    "test:ui": "jest --config jest.config.js --testPathIgnorePatterns='integration/buildVerification'",
    "test:ui:watch": "jest --config jest.config.js --watch --testPathIgnorePatterns='integration/buildVerification'",
    "test:ui:coverage": "jest --config jest.config.js --coverage --testPathIgnorePatterns='integration/buildVerification'",
    "test:build": "jest --config jest.config.js --testPathPattern='integration/buildVerification' --runInBand",
    "test:all": "npm run test && npm run test:ui && npm run test:build",
    "dev": "webpack serve --mode development",
    "anvil": "anvil --chain-id 1337",
    "deploy:local": "forge script script/Deploy.s.sol:DeployConferenceLocal --broadcast --rpc-url http://localhost:8545 --sender 0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266 --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80",
    "build-contracts": "forge build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makoto/blockparty.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/makoto/blockparty/issues"
  },
  "homepage": "https://github.com/makoto/blockparty#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@ensdomains/ens": "^0.6.2",
    "@ensdomains/resolver": "^0.3.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "axios": "^1.9.0",
    "copy-webpack-plugin": "^13.0.0",
    "crypto-browserify": "^3.12.1",
